{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Posts",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "subtitle": {
      "type": "string"
    },
    "featured_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "lounge": {
      "model": "lounge"
    },
    "thumbnail_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "slug": {
      "type": "string",
      "unique": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Flight",
        "Hotel",
        "Lounge"
      ],
      "required": false
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "post.carousel",
        "post.content-block"
      ]
    }
  }
}
